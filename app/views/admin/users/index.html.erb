<div class="col-md-6">
  <%= render 'admin/component/box_header', title: 'List users', button: 'Add new', icon: 'plus' %>

  <div class="content-box-large box-with-header">
    <table class="table table-hover">
      <thead>
        <tr>
          <th>SYSID</th>
          <th>Email</th>
          <th>Role</th>
          <th>Action</th>
        </tr>
      </thead>
      <tbody>
        <%= render partial: 'user', collection: @users %>
      </tbody>
    </table>
  </div>
</div>
